<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Arjun Kalburgi TMA3 @ViewData["Title"] - part1</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/materializecss/materialize.min.css" />
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" href="https://raw.githubusercontent.com/askalburgi/COMP466TMA3/master/part4/wwwroot/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css" type="text/css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css"
              asp-fallback-href="~/lib/materializecss/materializecss.min.css" type="text/css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="https://raw.githubusercontent.com/askalburgi/COMP466TMA3/master/part4/wwwroot/css/site.css" 
              asp-append-version="true" type="text/css" />
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    </environment>
</head>
<body>
    <nav class="light-blue lighten-1" role="navigation">
        <div class="nav-wrapper container">
            <a id="logo-container" href="index.php" class="brand-logo left"><i class="material-icons">flash_on</i></a>

            <ul class="right">
                <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-area="" asp-controller="Home" asp-action="SignIn">Sign In/Out</a></li>
            </ul>
        </div>
    </nav>

    <div class="main">@RenderBody()</div>
        
    <footer class="page-footer orange">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h5 class="white-text">Your computer store</h5>
                    <h4 class="white-text">
                        <i class="material-icons">flash_on</i>
                        <i class="material-icons">flash_on</i>
                        <i class="material-icons">flash_on</i>
                        <i class="material-icons">flash_on</i>
                        <i class="material-icons">flash_on</i>
                        <i class="material-icons">flash_on</i>
                    </h4>


                </div>
                <div class="col l3 s12">
                    <h5 class="white-text">Links</h5>
                    <ul>
                        <li><a class="white-text" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
                        <li><a class="white-text" asp-area="" asp-controller="Home" asp-action="About">Feedback</a></li>
                    </ul>
                </div>
                <div class="col l3 s12">
                    <h5 class="white-text">Add more stock</h5>
                    <ul>
                        <li><a class="white-text" asp-area="" asp-controller="Home" asp-action="Indexcopy">Indexcopy</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                Made by <a class="orange-text text-lighten-3" href="http://www.arjunkalburgi.com">Arjun</a>
            </div>
        </div>
    </footer>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/lib/materializecss/materialize.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js" 
            asp-fallback-src="~/lib/materializecss/materializecss.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
            integrity="sha384-C5yXM3HiWa6a8kI2Jd4LuuwmOGAVgJw0YSmuRXN+PLT5Jln26ddUnPQxkInM/w2x" 
            crossorigin="anonymous">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
